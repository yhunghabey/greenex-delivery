    <link rel="icon" href="images/fav-icon.svg" type="image/svg" />
    <title>Ker Active</title>
  </head>

  <body class="">
    <header style="background:#AAA79E;" class="">
      <section class="">
      <nav class="section">
        <div class="d-flex align-items-center">
          <img
            onclick="location.href='/'"
            src="images/Brand-logo.svg"
            alt=""
            class="brand-logo"
          />
        </div>
  
        <div class="nav-links" id="nav-links">
          <ul class="navbar-links">
            <li onclick="location.href='/'" class="">HOME</li>
            <li onclick="location.href='/trainers'" class="">TRAINERS</li>
            <li onclick="location.href='/contact'" class="">CONTACT</li>
          </ul>
        </div>
      </nav>

      <div style="padding-top: 15rem;" class="sub2">
        <h1 class="all-trainers">ALL TRAINERS</h1>

        <div id="web" class="d-flex align-items-center justify-content-between">
          <div class="py-5 d-flex align-items-center gap-5">
            <button class="btn-custom active">
              Filter
            </button>
          <form action="/filter" method="get">
            <select class="dropdown">
              <option name="searchTerm" value="Services">Services</option>
              <option name="searchTerm" value="Nutritionist">Nutritionist</option>
              <option name="searchTerm" value="Tennis">Tennis</option>
              <option name="searchTerm" value="Dietician">Dietician</option>
              <option name="searchTerm" value="Squash">Squash</option>
              <option name="searchTerm" value="Table Tennis">Table Tennis</option>
              <option name="searchTerm" value="Yoga Instructor">Yoga Instructor</option>
              <option name="searchTerm" value="Karate Instructor">Karate Instructor</option>
              <option name="searchTerm" value="Volleyball">Volleyball</option>
            </select>
            <select class="dropdown acti">
              <option name="searchTerm" value="Location">Location</option>
              <option name="searchTerm" value="Lagos">Lagos</option>
              <option name="searchTerm" value="Ikeja">Ikeja</option>
              <option name="searchTerm" value="Surulere">Surulere</option>
              <option name="searchTerm" value="Lekki">Lekki</option>
              <option name="searchTerm" value="Ajah">Ajah</option>
              <option name="searchTerm" value="Magodo">Magodo</option>
              <option name="searchTerm" value="Victorial Island">Victorial Island</option>
              <option name="searchTerm" value="Ojodu">Ojodu</option>
            </select>
          </form>
          </div>

          <div class="btn-custom bg-body-secondary  border-0 mt-5">
            <input class="trainer-search bg-body-secondary " type="search" placeholder="Search"/>
              <i class="fa-solid fa-magnifying-glass ms-5 fa-"></i>
          </div>
        </div>

        <div class="mobile split-mobile">
          <button class="btn-custom active">
            All
          </button>
          <button class="btn-custom">
            Category
          </button>
          <button class="btn-custom">
            <span class="me-5">Search</span> <i class="fa-solid fa-magnifying-glass ms-1"></i>
          </button>
        </div>
      </div>
    </section>
    </header>

   <section style="background:#AAA79E;" class="sect">
