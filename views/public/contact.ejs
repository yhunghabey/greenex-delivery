    <header
      style="
        background: url(images/contact-bg.png);
        height: 100vh;
        width: 100%;
        background-size: cover;
        background-position: center;
        background-repeat: no-repeat;
        position: relative;
        z-index: -2;
      "
      class=""
    >
      <section class="">
        <nav class="section">
          <div class="d-flex align-items-center">
            <img
              onclick="location.href='/'"
              src="images/Brand-logo.svg"
              alt=""
              class="brand-logo"
            />
          </div>

          <div class="nav-links" id="nav-links">
            <ul class="navbar-links">
              <li onclick="location.href='/'" class="">HOME</li>
              <li onclick="location.href='/trainers'" class="">TRAINERS</li>
              <li onclick="location.href='/contact'" class="">CONTACT</li>
            </ul>
          </div>
        </nav>
      </section>
    </header>

    <section
      style="
        background: url(/images/paper-edge.png);
        background-repeat: no-repeat;
        background-size: cover;
        background-position: center;
        padding-bottom: 25rem;
      "
