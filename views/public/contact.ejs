          <div class="col-md">
            <form action="/submitForm" class="" method="post">
              <input
                type="text"
                id="fullname"
                name="fullname"
                onblur="validateName()"
                class="w-100 p-4 px-5 rounded-pill text-dark bg-light mb-4"
                placeholder="FULLNAME"
              />
              <div id="fullnameError" class="error"></div>

              <input
                type="text"
                id="email"
                name="email"
                class="w-100 p-4 px-5 rounded-pill text-dark bg-light mb-4"
                placeholder="EMAIL"
                onblur="isValidEmail()"
              />
              <div id="emailError" class="error"></div>

              <input
                type="text"
                id="subject"
                name="subject"
                class="w-100 p-4 px-5 rounded-pill text-dark bg-light mb-4"
                placeholder="SUBJECT"
                onblur="validateSubject()"
              />
              <div id="subjectError" class="error"></div>

              <textarea
                style="border-radius: 20px"
                class="p-4 px-5 w-100 mb-4"
                name="message"
                id="message"
                cols="30"
                rows="10"
                placeholder="MESSAGE"
                onblur="validateMessage()"
              ></textarea>
              <div id="messageError" class="error"></div>

              <button class="btn-custom-2 text-center w-100">
                <span class="ms-auto fw-bold">SUBMIT</span>
                <i class="fa-solid fa-arrow-right ms-auto"></i>
              </button>
            </form>
          </div>
        </div>
