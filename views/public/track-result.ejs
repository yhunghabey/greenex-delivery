<!DOCTYPE html>
<html lang="en">
  <head>
    <title>GreenEx Delivery - Transportation, Cargo & Logistics</title>
    <link rel="icon" href="images/icon.webp" type="image/gif" sizes="16x16" />
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta
      content="LogiXpress - Transportation, Cargo & Logistics HTML Template"
      name="description"
    />
    <meta content="" name="keywords" />
    <meta content="" name="author" />
    <!-- CSS Files
    ================================================== -->
    <link
      href="css/bootstrap.min.css"
      rel="stylesheet"
      type="text/css"
      id="bootstrap"
    />
    <link href="css/plugins.css" rel="stylesheet" type="text/css" />
    <link href="css/swiper.css" rel="stylesheet" type="text/css" />
    <link href="css/swiper-custom-1.css" rel="stylesheet" type="text/css" />
    <link href="css/style.css" rel="stylesheet" type="text/css" />
    <link href="css/coloring.css" rel="stylesheet" type="text/css" />
    <!-- color scheme -->
    <link
      id="colors"
      href="css/colors/scheme-01.css"
      rel="stylesheet"
      type="text/css"
    />
  </head>

  <body>
    <!-- ðŸŒ Language selector at top of page -->
    <div
      id="google_translate_element"
      style="
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        background: #f8f9fa;
        padding: 8px 0;
        text-align: right;
        z-index: 9999;
        border-bottom: 1px solid #ddd;
      "
    ></div>

    <!-- Push down the rest of the content slightly -->
    <div style="margin-top: 50px"></div>

    <div id="wrapper">
      <a href="#" id="back-to-top"></a>

      <!-- page preloader begin -->
      <div id="de-loader"></div>
      <!-- page preloader close -->

      <!-- header begin -->
      <header class="transparent">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <div class="de-flex sm-pt10">
                <div class="de-flex-col">
                  <!-- logo begin -->
                  <div id="logo">
                    <a href="/">
                      <img
                        class="logo-main"
                        src="images/greenexdel.png"
                        alt=""
                      />
                      <img
                        class="logo-scroll"
                        src="images/logo-white-2.webp"
                        alt=""
                      />
                      <img
                        class="logo-mobile"
                        src="images/logo-white-2.webp"
                        alt=""
                      />
                    </a>
                  </div>
                  <!-- logo end -->
                </div>
                <div class="de-flex-col header-col-mid">
                  <!-- mainemenu begin -->
                  <ul id="mainmenu">
                    <li><a class="menu-item" href="/">Home</a></li>
                    <li>
                      <a class="menu-item" href="/track">Tracking</a>
                    </li>
                    <li>
                      <a class="menu-item" href="/service">Services</a>
                    </li>
                    <li>
                      <a class="menu-item" href="/about-us">About Us</a>
                    </li>
                    <li>
                      <a class="menu-item" href="#section-why-choose-us"
                        >Why Choose Us</a
                      >
                    </li>
                    <li><a class="menu-item" href="/contact">Contact</a></li>
                  </ul>
                  <!-- mainmenu end -->
                </div>
                <div class="de-flex-col">
                  <div class="menu_side_area">
                    <a href="/booking" class="btn-main btn-line fx-slide"
                      ><span>Book Shipment</span></a
                    >
                    <span id="menu-btn"></span>
                  </div>

                  <div id="btn-extra">
                    <span></span>
                    <span></span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </header>
      <!-- header close -->
      <!-- content begin -->
      <div class="no-bottom no-top" id="content">
        <div id="top"></div>

        <section
          id="subheader"
          class="text-light sm-mt-90 relative rounded-1 overflow-hidden m-3"
          data-bgimage="url(images/background/7.webp) center"
        >
          <div class="container relative z-2">
            <div class="row gy-4 gx-5 align-items-center">
              <div class="col-lg-12">
                <h1 class="split">Track Result</h1>
                <ul class="crumb wow fadeInUp">
                  <li><a href="index.html">Home</a></li>
                  <li class="active">Track Result</li>
                </ul>
              </div>
            </div>
          </div>
          <div class="gradient-edge-bottom color op-7 h-80"></div>
          <div class="sw-overlay op-7"></div>
        </section>

        <!-- Delivery Information Section -->
        <section class="mt-5 mb-5">
          <div class="container">
            <div class="row justify-content-center">
              <div class="col-lg-8">
                <% if (booking) { %>
                <div class="card shadow-sm p-4 border-0">
                  <h3 class="text-center mb-4">Delivery Information</h3>

                  <div class="row">
                    <!-- Shipper Information -->
                    <div class="col-md-6 mb-4">
                      <h5 class="fw-bold text-uppercase">
                        Shipper Information
                      </h5>
                      <p>
                        <strong>Name:</strong> <%= booking.shippersName || 'N/A'
                        %>
                      </p>
                      <p>
                        <strong>Address:</strong> <%= booking.shippersAddress ||
                        'N/A' %>
                      </p>
                      <p>
                        <strong>Phone:</strong> <%= booking.shippersPhoneNo ||
                        'N/A' %>
                      </p>
                      <p>
                        <strong>Email:</strong> <%= booking.shippersEmail ||
                        'N/A' %>
                      </p>
                    </div>

                    <!-- Receiver Information -->
                    <div class="col-md-6 mb-4">
                      <h5 class="fw-bold text-uppercase">
                        Receiver Information
                      </h5>
                      <p>
                        <strong>Name:</strong> <%= booking.receiversName ||
                        'N/A' %>
                      </p>
                      <p>
                        <strong>Address:</strong> <%= booking.deliveryAddress ||
                        'N/A' %>
                      </p>
                      <p>
                        <strong>Phone:</strong> <%= booking.receiversPhoneNo ||
                        'N/A' %>
                      </p>
                      <p>
                        <strong>Email:</strong> <%= booking.receiversEmail ||
                        'N/A' %>
                      </p>
                    </div>
                  </div>

                  <% if (booking.additionalNote) { %>
                  <div class="mt-2 mb-0">
                    <h5 class="fw-bold text-uppercase mb-1">Additional Note</h5>
                    <p class="fst-italic mb-0"><%= booking.additionalNote %></p>
                  </div>
                  <hr
                    class="my-2"
                    style="opacity: 0.15; margin-top: 6px; margin-bottom: 6px"
                  />
                  <% } %>
                </div>
                <% } %>
              </div>
            </div>
          </div>
        </section>

        <section>
          <div class="container">
            <div class="row justify-content-center">
              <div class="col-lg-8">
                <div class="text-center">
                  <% if (message) { %>
                  <h3><%= message %></h3>
                  <% } else { %>
                  <h3>Tracking for Package ID #<%= booking.trackingId %></h3>
                  <% } %>
                </div>

                <div class="spacer-single"></div>

                <div class="de-timeline">
                  <div
                    class="<%= bookingStatus === 'Order Placed' ? 'd-item active' : 'd-item' %>"
                  >
                    <div class="d-text">
                      <div class="d-icon"><i class="fa fa-check"></i></div>
                      <h4>Order Placed</h4>
                      Shipment created by sender
                    </div>
                  </div>

                  <div
                    class="<%= bookingStatus === 'Picked Up' ? 'd-item active' : 'd-item' %>"
                  >
                    <div class="d-text">
                      <div class="d-icon"><i class="fa fa-check"></i></div>
                      <h4>Picked Up</h4>
                      Package collected from sender
                    </div>
                    <div class="d-date">
                      <%= new Date(booking.pickupDate).toLocaleString('en-GB', {
                      day: '2-digit', month: 'short', year: 'numeric', hour:
                      '2-digit', minute: '2-digit', hour12: true }) %>
                    </div>
                  </div>

                  <div
                    class="<%= bookingStatus === 'In Transit' ? 'd-item active' : 'd-item' %>"
                  >
                    <div class="d-text">
                      <div class="d-icon"><i class="fa fa-check"></i></div>
                      <h4>In Transit</h4>
                      Moving through logistics network
                    </div>
                  </div>

                  <div
                    class="<%= bookingStatus === 'Arrived At Hub' ? 'd-item active' : 'd-item' %>"
                  >
                    <div class="d-text">
                      <div class="d-icon"><i class="fa fa-check"></i></div>
                      <h4>Arrived at Hub</h4>
                      Package reached regional distribution center
                    </div>
                  </div>

                  <div
                    class="<%= bookingStatus === 'Out For Delivery' ? 'd-item active' : 'd-item' %>"
                  >
                    <div class="d-text">
                      <div class="d-icon"><i class="fa fa-check"></i></div>
                      <h4>Out for Delivery</h4>
                      Courier is delivering the package
                    </div>
                  </div>

                  <div
                    class="<%= bookingStatus === 'Delivery Attempt' ? 'd-item active' : 'd-item' %>"
                  >
                    <div class="d-text">
                      <div class="d-icon">
                        <i class="fa fa-check"></i>
                      </div>
                      <h4>Delivery Attempted</h4>
                      Courier attempted delivery
                    </div>
                    <div class="d-date">
                      <%= new
                      Date(booking.deliveryDeadline).toLocaleString('en-GB', {
                      day: '2-digit', month: 'short', year: 'numeric', hour:
                      '2-digit', minute: '2-digit', hour12: true }) %>
                    </div>
                  </div>

                  <div
                    class="<%= bookingStatus === 'Delivered' ? 'd-item active' : 'd-item' %>"
                  >
                    <div class="d-text">
                      <div class="d-icon"><i class="fa fa-check"></i></div>
                      <h4>Delivered</h4>
                      Package successfully delivered
                    </div>
                    <div class="d-date">
                      <%= new
                      Date(booking.deliveryDeadline).toLocaleString('en-GB', {
                      day: '2-digit', month: 'short', year: 'numeric', hour:
                      '2-digit', minute: '2-digit', hour12: true }) %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
      </div>
      <!-- content close -->

      <!-- footer begin -->
      <footer class="text-light section-dark">
        <div class="container">
          <div class="row g-4 justify-content-between">
            <div class="col-md-6">
              <img src="images/greenexdel.png" class="w-170px mb-2" alt="" />
              <div class="spacer-single"></div>
              <div class="row g-4">
                <div class="col-md-6">
                  <div class="widget">
                    <h5>Services</h5>
                    <ul>
                      <li><a href="/service">Trucking</a></li>
                      <li><a href="/service">Air Freight</a></li>
                      <li><a href="/service">Ship Freight</a></li>
                      <li><a href="/service">Rail Freight</a></li>
                      <li><a href="/service">Warehousing</a></li>
                      <li>
                        <a href="/service">Customs Brokerage</a>
                      </li>
                    </ul>
                  </div>
                </div>

                <div class="col-md-6">
                  <div class="widget">
                    <h5>Company</h5>
                    <ul>
                      <li><a href="/">Home</a></li>
                      <li><a href="/about-us">About Us</a></li>
                      <li><a href="/about-us">Our Team</a></li>
                      <li><a href="/about-us">Careers</a></li>
                      <li><a href="/">Blog</a></li>
                      <li><a href="/contact">Contact</a></li>
                    </ul>
                  </div>
                </div>
              </div>

              <div class="social-icons mb-sm-30 text-center">
                <a href="#"><i class="fa-brands fa-facebook-f"></i></a>
                <a href="#"><i class="fa-brands fa-x-twitter"></i></a>
                <a href="#"><i class="fa-brands fa-instagram"></i></a>
                <a href="#"><i class="fa-brands fa-youtube"></i></a>
                <a href="#"><i class="fa-brands fa-whatsapp"></i></a>
              </div>
            </div>

            <div class="col-md-6">
              <div class="d-flex align-items-center justify-content-between">
                <h2>Get in Touch</h2>
                <img
                  src="images/ui/up-right-arrow.webp"
                  class="w-60px op-5"
                  alt=""
                />
              </div>

              <div class="widget">
                <div class="op-5 fs-15">Email</div>
                <h3>contact@greenexdelivery.com</h3>

                <div class="spacer-20"></div>

                <div class="op-5 fs-15">Phone</div>
                <h3>+1 (458) 250-9783</h3>

                <div class="spacer-20"></div>

                <div class="op-5 fs-15">Office Location</div>
                <h3>3500 E. 17th Ave, Eugene, OR 97403</h3>

                <div class="spacer-20"></div>
              </div>
            </div>
          </div>
        </div>
        <div class="subfooter">
          <div class="container">
            <div class="row">
              <div class="col-md-12 text-center">
                Copyright 2025 GreenEx Delivery
              </div>
            </div>
          </div>
        </div>
      </footer>
      <!-- footer close -->
    </div>

    <!-- Javascript Files
    ================================================== -->
    <script src="js/plugins.js"></script>
    <script src="js/designesia.js"></script>
    <script src="js/swiper.js"></script>
    <script src="js/custom-swiper-1.js"></script>
    <script src="js/custom-marquee.js"></script>
    <script type="text/javascript">
      function googleTranslateElementInit() {
        new google.translate.TranslateElement(
          {
            pageLanguage: "en", // default language
            includedLanguages: "en,fr,yo,es,de,ar,ja,zu,zh-CN,pt", // customize languages
            layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
          },
          "google_translate_element"
        );
      }
    </script>

    <script src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>

    <style>
      /* Optional: hide Google branding banner */
      .goog-te-banner-frame.skiptranslate,
      .goog-logo-link {
        display: none !important;
      }
      body {
        top: 0px !important;
      }
    </style>
  </body>
</html>
